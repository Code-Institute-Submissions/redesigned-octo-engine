<div id="container">
    {{#if isVideoExpanded}}
      <div class="box" id="one">
      </div>
      <div class="box" id="home-video">
        <div id="x-button">
          <img src="assets/images/x-mark-5-512.png" class="square-x" id="x-icon">
        </div>
        <video id="home-video" controls autoplay>
          <source src="assets/videos/macaque-monkey-on-tree-jumping-climbing-playing_r7u2ngmxl__WL.mp4" type="video/mp4">
            <!-- <source src="movie.ogg" type="video/ogg"> -->
            Your browser does not support the video tag.
        </video>
      </div>
      <div class="box" id="two">
      </div>
    {{else}}
      <div id="home-button">
        <img id="home-play-icon" src="assets/images/icons8-play-button_white.png">
      </div>
    {{/if}}
</div>
